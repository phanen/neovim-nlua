{
  "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
  "diagnostics": {
    "disable": [
      "unnecessary-if",
      "undefined-field",
      "access-invisible"
    ],
    "enable": true
  },
  "runtime": {
    "requirePattern": [
      "lua/?.lua",
      "lua/?/init.lua"
    ],
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME"
    ]
  }
}